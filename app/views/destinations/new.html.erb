 <%= form_for @destination do |f| %>
   <%= f.label :address %>
   <%= f.text_field :address %>

   <%= f.label :city %>
   <%= f.text_field :city %>

   <%= f.label :state %>
   <%= f.text_field :state %>

   <%= f.label :full_street_address %>
   <%= f.text_field :full_street_address %>

   <%= f.submit "Save" %>
 <% end %>
